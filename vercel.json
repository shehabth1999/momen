{
  "version": 2,
  "builds": [
    {
      "src": "core/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.8"
      }
    }
  ],
  "functions": {
    "core/wsgi.py": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "core/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "core/wsgi.py"
    }
  ]
}
