{
    "version": 2,
    "builds": [
      {
        "src": "core/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "project_name/wsgi.py"
      }
    ]
  }
  